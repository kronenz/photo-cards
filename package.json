{
  "name": "holographic-card-community",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "build:optimized": "NODE_ENV=production vite build",
    "preview": "vite preview --host",
    "build:vercel": "npm run check && npm run build:optimized",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:holographic": "vitest run src/lib/holographic/__tests__/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:cross-browser": "playwright test tests/e2e/cross-browser.spec.ts",
    "test:mobile": "playwright test tests/e2e/mobile-touch.spec.ts",
    "test:performance": "playwright test tests/e2e/performance.spec.ts",
    "setup:pocketbase": "node scripts/setup-pocketbase.js",
    "setup:collections": "node scripts/setup-collections.js",
    "setup:all": "npm run setup:pocketbase && npm run setup:collections",
    "analyze:css": "node scripts/optimize-css.js",
    "optimize:css": "npm run build:optimized && npm run analyze:css",
    "pocketbase": "cd pocketbase && ./pocketbase serve --http=127.0.0.1:8090",
    "pocketbase:win": "cd pocketbase && pocketbase.exe serve --http=127.0.0.1:8090",
    "dev:full": "concurrently \"npm run pocketbase:win\" \"npm run dev\"",
    "dev:full:unix": "concurrently \"npm run pocketbase\" \"npm run dev\"",
    "tunnel": "ngrok http 5173",
    "dev:tunnel": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "start": "npm run dev:full",
    "docker:setup": "./scripts/docker-setup.sh dev",
    "docker:setup:prod": "./scripts/docker-setup.sh prod",
    "docker:stop": "./scripts/docker-stop.sh dev",
    "docker:stop:prod": "./scripts/docker-stop.sh prod",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:logs:prod": "docker-compose logs -f",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart",
    "docker:restart:prod": "docker-compose restart",
    "docker:minio:cluster": "docker-compose -f docker-compose.minio-cluster.yml up -d",
    "docker:minio:cluster:stop": "docker-compose -f docker-compose.minio-cluster.yml down",
    "docker:minio:cluster:logs": "docker-compose -f docker-compose.minio-cluster.yml logs -f"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@auth/sveltekit": "^1.10.0",
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@sveltejs/adapter-vercel": "^5.10.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/svelte-virtual": "^3.13.12",
    "bits-ui": "^0.22.0",
    "jszip": "^3.10.1",
    "phash-js": "^0.3.0",
    "pocketbase": "^0.21.4",
    "sharp": "^0.33.0",
    "svelte-gestures": "^5.2.2"
  },
  "devDependencies": {
    "@percy/cli": "^1.31.2",
    "@percy/playwright": "^1.0.9",
    "@playwright/test": "^1.40.0",
    "@sveltejs/adapter-vercel": "^5.4.4",
    "@sveltejs/kit": "^2.5.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@types/jszip": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.10.3",
    "axe-playwright": "^2.2.2",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "vite": "^5.3.5",
    "vitest": "^3.2.4"
  },
  "description": "This is a repository holder for the Pokemon Cards CSS Holographic effect.",
  "main": "svelte.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kronenz/pokemon-cards-css.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kronenz/pokemon-cards-css/issues"
  },
  "homepage": "https://github.com/kronenz/pokemon-cards-css#readme"
}
